{
  "name": "okAPI Game",
  "servePort": 34872,
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "Assets": {
        "$className": "Folder",
        "Models": {
          "$path": "assets/models"
        },
        "Animations": {
          "$path": "assets/animations"
        },
        "Textures": {
          "$path": "assets/textures"
        },
        "Sounds": {
          "$path": "assets/sounds"
        },
        "UI": {
          "$className": "Folder",
          "Images": {
            "$path": "assets/ui/images"
          },
          "Icons": {
            "$path": "assets/ui/icons"
          }
        }
      }
    },
    "ServerScriptService": {
      "$path": "src/server"
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client"
      }
    },
    "StarterGui": {
      "$path": "assets/ui"
    },
    "Workspace": {
      "Models": {
        "$path": "assets/workspace_models"
      }
    }
  }
}
